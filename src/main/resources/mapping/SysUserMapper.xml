<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.geek.conding.mapper.SysUserMapper">

    <resultMap id="BaseResultMap" type="com.geek.conding.model.rds.SysUserDTO">
        <result column="id" jdbcType="INTEGER" property="id"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
    </resultMap>

    <select id="queryUserAll" resultMap="BaseResultMap">
        SELECT
        id,name
        FROM sys_user
    </select>

    <select id="queryUserAllByPage" resultMap="BaseResultMap">
        SELECT
        id,name
        FROM sys_user
    </select>

</mapper>